<h1>Listing photos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Url</th>
    <th>Filter</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.name %></td>
    <td><%= photo.description %></td>
    <td><%= photo.url %></td>
    <td><%= photo.filter %></td>
    <td><%= link_to 'Show', [photo.user, photo] %></td>
    <td><%= link_to 'Edit', edit_user_photo_path(photo.user, photo) %></td>
    <td><%= link_to 'Destroy', [photo.user, photo], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_user_photo_path(params[:user_id]) %>
